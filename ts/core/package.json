{
  "name": "memoer",
  "version": "0.1.5",
  "description": "Memory management system for LLMs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/index.d.ts",
      "require": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "memory",
    "ai",
    "conversation",
    "context"
  ],
  "author": "Ge Li",
  "license": "Apache-2.0",
  "dependencies": {
    "ai": ">=2.0.0"
  },
  "peerDependencies": {
    "ai": ">=2.0.0"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
